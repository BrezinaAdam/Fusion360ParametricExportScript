{
  "outputDirectory": "export",
  "exportOptions": {
    "fileType": "STL",
    "stlQuality": "High",
    "fileNameTemplate": "{bodyName}-s{Size}-a{HandleAngle}-th{Thickness}mm-w{HandleWidth}mm",
    "forceRecompute": true
  },
  "bodiesToExport": [
    "Handle",
    "Handle-wScrewHole"
  ],
  "parametersToIterate": {
    "Size": {
      "variants": [2, 3, 4, 5],
      "grouping": true
    },
    "HandleAngle": {
      "variants": [105, 115, 125, 135]
    },
    "HandleWidth": {	
      "variants": [16, 23],
      "grouping": true
    },
    "Thickness": {
      "variants": [5, 7, 9]
    }
  }
}
